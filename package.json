{
  "name": "nataliafomina.com",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n Next,~TS~ -c bgWhite.black,bgBlue.black \"next dev\" \"npm run ts:watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty",
    "ts:watch": "tsc --noEmit --incremental --preserveWatchOutput --pretty --watch",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "prepare:playwright": "playwright install && playwright install-deps"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.5",
    "@emotion/react": "11.8.1",
    "@emotion/styled": "11.8.1",
    "classnames": "2.3.1",
    "framer-motion": "6.2.8",
    "graphql": "16.3.0",
    "graphql-request": "4.0.0",
    "handlebars": "4.7.7",
    "lodash": "4.17.21",
    "markdown-to-jsx": "7.1.6",
    "next": "12.1.0",
    "playwright-aws-lambda": "0.7.0",
    "playwright-core": "1.23.2",
    "polished": "4.1.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "sass": "1.52.3",
    "showdown": "2.0.1",
    "swr": "1.2.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/showdown": "1.9.4",
    "@types/lodash": "4.14.182",
    "concurrently": "7.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
